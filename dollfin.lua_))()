-- –ê–≤—Ç–æ—Ä: ChatGPT | –†–∞–±–æ—Ç–∞–µ—Ç —Ç–æ–ª—å–∫–æ –≤ —Ç–≤–æ–µ–π –∏–≥—Ä–µ
-- –≠—Ñ—Ñ–µ–∫—Ç—ã: –±—ã—Å—Ç—Ä–æ–µ –¥–≤–∏–∂–µ–Ω–∏–µ, –ø–æ–¥—Å–≤–µ—Ç–∫–∞ –∏–≥—Ä–æ–∫–æ–≤, –ø–ª–∞—Ç—Ñ–æ—Ä–º–∞, –º–µ–Ω—é –∏ –¥–µ–ª—å—Ñ–∏–Ω ü©∑

-- === –ù–ê–°–¢–†–û–ô–ö–ò ===
local walkSpeedBoost = 50
local menuColor = Color3.fromRGB(255, 150, 200) -- —Ä–æ–∑–æ–≤—ã–π
local dolphinImage = "rbxassetid://7486774049" -- –º–æ–∂–µ—à—å –∑–∞–º–µ–Ω–∏—Ç—å

-- === –°–û–ó–î–ê–ù–ò–ï –ú–ï–ù–Æ ===
local player = game.Players.LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")

-- –ü—Ä–æ–≤–µ—Ä–∫–∞, —á—Ç–æ–±—ã –Ω–µ —Å–æ–∑–¥–∞–≤–∞—Ç—å –ø–æ–≤—Ç–æ—Ä–Ω–æ
if playerGui:FindFirstChild("DollfinUI") then
    playerGui.DollfinUI:Destroy()
end

local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Name = "DollfinUI"
ScreenGui.ResetOnSpawn = false
ScreenGui.Parent = playerGui

local Frame = Instance.new("Frame")
Frame.Size = UDim2.new(0, 260, 0, 220)
Frame.Position = UDim2.new(0.5, -130, 0.5, -110)
Frame.BackgroundColor3 = menuColor
Frame.Active = true
Frame.Draggable = true
Frame.Visible = true
Frame.Parent = ScreenGui

local Title = Instance.new("TextLabel")
Title.Text = "üê¨ Dollfin Script"
Title.Size = UDim2.new(1, 0, 0, 40)
Title.BackgroundTransparency = 1
Title.TextColor3 = Color3.fromRGB(255, 255, 255)
Title.Font = Enum.Font.GothamBold
Title.TextSize = 24
Title.Parent = Frame

-- === –ö–ù–û–ü–ö–ò ===
local function makeButton(name, y, action)
    local btn = Instance.new("TextButton")
    btn.Text = name
    btn.Size = UDim2.new(1, -20, 0, 40)
    btn.Position = UDim2.new(0, 10, 0, y)
    btn.BackgroundColor3 = Color3.fromRGB(255, 200, 230)
    btn.TextColor3 = Color3.fromRGB(0, 0, 0)
    btn.Font = Enum.Font.Gotham
    btn.TextSize = 18
    btn.Parent = Frame
    btn.MouseButton1Click:Connect(action)
    return btn
end

local character = player.Character or player.CharacterAdded:Wait()

-- –ë—ã—Å—Ç—Ä—ã–π –±–µ–≥
makeButton("üí® –ë—ã—Å—Ç—Ä—ã–π –±–µ–≥", 50, function()
    local hum = character:FindFirstChildOfClass("Humanoid")
    if hum then
        if hum.WalkSpeed == 16 then
            hum.WalkSpeed = walkSpeedBoost
        else
            hum.WalkSpeed = 16
        end
    end
end)

-- –ü–æ–¥—Å–≤–µ—Ç–∫–∞ –∏–≥—Ä–æ–∫–æ–≤ (ESP)
local espOn = false
makeButton("‚ú® –ü–æ–¥—Å–≤–µ—Ç–∫–∞ –∏–≥—Ä–æ–∫–æ–≤", 100, function()
    espOn = not espOn
    for _, plr in ipairs(game.Players:GetPlayers()) do
        if plr ~= player and plr.Character and plr.Character:FindFirstChild("Head") then
            if espOn then
                -- –£–¥–∞–ª–∏—Ç—å —Å—Ç–∞—Ä—É—é ESP, –µ—Å–ª–∏ –µ—Å—Ç—å
                local oldEsp = plr.Character.Head:FindFirstChild("ESP")
                if oldEsp then oldEsp:Destroy() end

                local billboard = Instance.new("BillboardGui")
                billboard.Name = "ESP"
                billboard.Size = UDim2.new(0, 100, 0, 30)
                billboard.AlwaysOnTop = true
                billboard.Parent = plr.Character.Head
                local label = Instance.new("TextLabel")
                label.Text = plr.Name
                label.Size = UDim2.new(1, 0, 1, 0)
                label.BackgroundTransparency = 1
                label.TextColor3 = Color3.fromRGB(255, 255, 255)
                label.Font = Enum.Font.GothamBold
                label.Parent = billboard
            else
                local esp = plr.Character.Head:FindFirstChild("ESP")
                if esp then esp:Destroy() end
            end
        end
    end
end)

-- –ü–ª–∞—Ç—Ñ–æ—Ä–º–∞ –ø–æ–¥ —Å–æ–±–æ–π
makeButton("‚¨ú –ü–ª–∞—Ç—Ñ–æ—Ä–º–∞", 150, function()
    if character and character:FindFirstChild("HumanoidRootPart") then
        local part = Instance.new("Part")
        part.Size = Vector3.new(10, 1, 10)
        part.Anchored = true
        part.Position = character.HumanoidRootPart.Position - Vector3.new(0, 4, 0)
        part.Color = Color3.fromRGB(255, 200, 230)
        part.Parent = workspace
    end
end)

-- –°–∫—Ä—ã—Ç—å –º–µ–Ω—é
makeButton("‚ùå –ó–∞–∫—Ä—ã—Ç—å –º–µ–Ω—é", 200, function()
    Frame.Visible = false

    -- –∫–∞—Ä—Ç–∏–Ω–∫–∞ –¥–µ–ª—å—Ñ–∏–Ω–∞ –ø—Ä–∏ –∑–∞–∫—Ä—ã—Ç–∏–∏
    local img = Instance.new("ImageLabel")
    img.Image = dolphinImage
    img.Size = UDim2.new(0, 200, 0, 200)
    img.Position = UDim2.new(0.5, -100, 0.5, -100)
    img.BackgroundTransparency = 1
    img.Parent = ScreenGui
    game:GetService("Debris"):AddItem(img, 3)
end)

-- === –°–æ–æ–±—â–µ–Ω–∏–µ –ø—Ä–∏ –∑–∞–≥—Ä—É–∑–∫–µ ===
local StarterGui = game:GetService("StarterGui")
StarterGui:SetCore("SendNotification", {
    Title = "üê¨ Dollfin Script Loaded!";
    Text = "–î–æ–±—Ä–æ –ø–æ–∂–∞–ª–æ–≤–∞—Ç—å! –ú–µ–Ω—é –∞–∫—Ç–∏–≤–∏—Ä–æ–≤–∞–Ω–æ";
    Duration = 5;
})